void main(){
    Class class1 = Class();

    Query q1 = Query("Szukana fraza");
    WebsiteWWW w1 = WebsiteWWW("http://portal.com");


    SearchResult sr1;
    if (w1.checkConnection()) {
        sr1 = q1.searchIn( w1);
        sr1.saveResultsToFile( "Nowyplik.txt");
    }
    else{
        File f1 = File("NowyError.txt");
        f1.writeText(w1.connectionStatus);
        f1.close();
    }
}